<!DOCTYPE html>
<html>
<head>
	<title></title>
	<meta charset="utf-8">

     
        <link rel="stylesheet" type="text/css" href="css/style.css"> 
  		<link rel="stylesheet" href="css/bootstrap.min.css">
        <script src="js/jquery-1.11.3.min.js"></script> 
</head>
<body>
	<!-- <%= title %> -->
	<% if(success) { %>
	<%= success %><% } %>
	<% if(error) { %>
	<%= error %><% } %>
	
	<form class="form-horizontal" method='post' action='/reg'>
		<div class="form-group">
			<label class="col-sm-2 control-label my-form-label">用户名</label>
			<div class="col-sm-10">
				<input type="text" class="form-control my-form-button" placeholder="username" name='username'>
			</div>
		</div>
		<div class="form-group">
			<label for="inputPassword3" class="col-sm-2 control-label my-form-label">密码</label>
			<div class="col-sm-10">
				<input type="password" class="form-control my-form-button" id="inputPassword3" placeholder="Password" name='password'>
			</div>
		</div>
		<div class="form-group">
			<label for="inputPassword3" class="col-sm-2 control-label my-form-label">确认密码</label>
			<div class="col-sm-10">
				<input type="password" class="form-control my-form-button" id="inputPassword3" placeholder="ConfiemPassword" name='passwordRepeat'>
			</div>
		</div>
		<div class="form-group">
			<label for="inputEmail3" class="col-sm-2 control-label my-form-label">邮箱</label>
			<div class="col-sm-10">
				<input type="email" class="form-control my-form-button" id="inputEmail3" placeholder="Email" name='name'>
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-offset-2 col-sm-2">
				<button type="submit" class="btn btn-success">提交</button>

			</div>
		</div>
	</form>
</body>
</html>



